{
  "heading": "Charged Up - 2023",
  "logo": "vv196x200.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text",
          "validation": {
            "comparison": "greater",
            "value": 0
          }
        },
        {

          "name": "Starting Position",
          "type": "positions",
          "file": "field.png"
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "name": "Left Community",
          "type": "checkbox"
        },
        {
          "name": "Co-Op Nodes",
          "type": "label",
          "align": "middle",
          "col": 2
        },

        {
          "name": "Scored Game Pieces",
          "type": "togglegrid",
          "prefix": "Auto",
          "width": 9,
          "height": 2,
          "colors": [
            "red",
            "green"
          ]
        },
        {
          "name": "Hybrid Nodes",
          "type": "togglegrid",
          "prefix": "Auto",
          "width": 9,
          "height": 1,
          "colors": [
            "black",
            "yellow",
            "purple"
          ]
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Charging Station",
          "type": "dropdown",
          "options": [
            "Not Attempted",
            "Engaged",
            "Docked",
            "Failed"
          ]
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Picked Up From:",
          "type": "heading",
          "col": 1,
          "colspan": 3,
          "align": "center"
        },

        {
          "name": "Ground",
          "type": "spinbox",
          "col": 2,
          "align": "left"
        },
        {
          "name": "Single Substation",
          "type": "spinbox",
          "col": 1,
          "align": "center"
        },
        {
          "name": "Double Station",
          "type": "spinbox",
          "col": 1,
          "align": "center"

        },
        {
          "name": "Scored to:",
          "type": "heading",
          "col": 1,
          "colspan": 3,
          "align": "center"
        },

        {
          "name": "Low",
          "type": "spinbox",
          "col": 1,
          "align": "center"
        },
        {
          "name": "Mid",
          "type": "spinbox",
          "col": 1,
          "align": "center"
        },
        {
          "name": "High",
          "type": "spinbox",
          "col": 1,
          "align": "center"

        },

        {
          "name": "Pieces Scored",
          "type": "label",
          "align": "center",
          "col": 2,
          "colspan": 3
        },


        {
          "name": "Defense Rating",
          "type": "dropdown",
          "colspan": 2,
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5 (Very Effective)"
          ]
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "Parking",
          "type": "radio",
          "options": [
            "Engaged",
            "Docked",
            "Parked",
            "None"
          ],
          "default": 3
        },
        {
          "name": "Failed Charging Station",
          "type": "checkbox"
        },
        {
          "name": "Robots Docked",
          "type": "spinbox",
          "max": 3
        },
        {
          "name": "Mechanical Failures",
          "type": "text"
        },
        {
          "name": "Comments",
          "type": "textarea"
        }
      ]
    }
  ]
}
